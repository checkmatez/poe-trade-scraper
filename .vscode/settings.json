{
  "deno.enable": true,
  "deno.lint": true,
  "deno.suggest.completeFunctionCalls": false,
  "deno.unstable": true,
  "deno.codeLens.referencesAllFunctions": false,
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  }
}
